
# Ignore dotenv files
.env

# Ignore packages files
*.7z
*.csv
*.dat
*.dmg
*.gz
*.iso
*.jar
*.rar
*.tar
*.zip

# Ignore compiled source
*.com
*.class
*.dll
*.exe
*.o
*.seed
*.so
*.pid

# Ignore logs and databases
*.log
*.sql
*.sqlite

# Ignore OS generated files
*~
._*
.DS_Store
.Trashes
.Spotlight-V100
Thumbs.db
ehthumbs.db

# Ignore IDE specific files
.idea
.vscode/*
!.vscode/tasks.json
!.vscode/launch.json
!.vscode/settings.json
!.vscode/extensions.json
*.code-workspace

# Ignore binary gettext files
*.po~
*.mo~

# Ignore node dependency directories
node_modules/

# Ignore Composer vendor directories
vendor/

# Ignore PHPCS/PHPUnit configurations
.phpcs.xml
.phpunit-watcher.yml
.phpunit.result.cache
